                innerhtml += "        <td>" + OSSAMonthlyCost + "</td>";
                innerhtml += "       <td>" + OSSPMonthlyCost + "</td>";
                innerhtml += "      <td>" + remark + "</td>";
                innerhtml += " <td class=\"action-td\">";
                innerhtml += "     <a href=\"javascript:void(0);\"  onclick=\"$(this).parents('tr').remove();comTotalPrice();\" class=\"btn btn-small\">";
