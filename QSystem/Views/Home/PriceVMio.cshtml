        //计量单位计算
        function change(control) {

            var currentTr = $(control).parents("tr");

            var ossaPrice = parseFloat($(currentTr).find("td:eq(5)").html());
            var osspPrice = parseFloat($(currentTr).find("td:eq(6)").html());
            var unit = parseInt($(control).val());



            var ossaMonth = unit * ossaPrice;
            var osspMonth = unit * osspPrice;

            if (!isNaN(ossaMonth)) {
                $(currentTr).find("td:eq(7)").html(ossaMonth);
            }

            if (!isNaN(osspMonth)) {
                $(currentTr).find("td:eq(8)").html(osspMonth);
            }

            comTotalPrice();

        }


        function comTotalPrice() {
            var price = 0.0;
            $(".table-bordered").each(function () {

                $(this).find("tr:gt(0)").each(function () {
                    price += parseFloat($(this).find("td:eq(7)").html());

                });

            });

            price = price.toFixed(2);
            $(".home-p").html("总计：" + price);
        }


